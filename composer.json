{
    "name": "vperyod/auth-handler",
    "description": "PSR7 Aura\\Auth Authentication Handler",
    "homepage":"https://github.com/vperyod/vperyod.auth-handler",
    "license": "MIT",
    "authors": [
        {
            "name": "jake johns",
            "email": "jake@jakejohns.net"
        }
    ],
    "require": {
        "php": ">=7.1",
        "aura/auth": "^2.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev":{
        "zendframework/zend-diactoros": "~2.0",
        "phpunit/phpunit": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Vperyod\\AuthHandler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vperyod\\AuthHandler\\":"tests/"
        }
    },
    "suggest": {
        "jnjxp/vk": "For authentication domain"
    }
}
